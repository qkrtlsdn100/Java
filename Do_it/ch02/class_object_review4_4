package review;

public class student_Test {
	public static void main(String[] args) {
		
		Student Sin = new Student("Sin", 19, 12000);
		Student Woo = new Student("Woo", 19, 5000);
		
		Bus bus = new Bus("112-5");
		Sin.bus(bus);
		Sin.showinfor();
		bus.showinfor();
		
		Subway subway = new Subway("5호선");
		Woo.subway(subway);
		Woo.showinfor();
		subway.showinfor();
	}
}
