package Person;

public class person {
	
	String PersonName;
	int age;
	String phonenumber;
	String email;
	
	public void showpersoninfor() {
		System.out.println("당신의 이름은 " + PersonName + " 나이는 " + age + " 전화번호는 " + phonenumber + " 이메일은 " + email);
	}
	
	public void person(String name, int age) {
		this.PersonName = name;
		this.age = age;
		
	}
	
	public void person(String name, int age, String ph, String email) {
		this.PersonName = name;
		this.age = age;
		this.phonenumber = ph;
		this.email = email;
		
	}
	
	public static void main(String[] args) {
		
		person personinfor = new person();
		
		personinfor.PersonName = "박신우";
		personinfor.age = 19;
		personinfor.phonenumber = "010 - **** - ****";
		personinfor.email = "qkrtlsdn100@daum.net";
		
		personinfor.showpersoninfor();
	}	
}
