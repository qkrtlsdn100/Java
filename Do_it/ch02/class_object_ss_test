package Dog;

public class DogTest {
	public static void main(String[] args) {
		
		Dog dog1 = new Dog();
		Dog dog2 = new Dog();
		
		dog1.Dogname = "짱구";
		dog1.Dogage = 3;
		
		dog2.Dogname = "흰둥이";
		dog2.Dogbreed = "진돗개";
		dog2.Dogage = 7;
		
		dog1.showdoginfor();
		dog2.showdoginfor();
		
	}
}
