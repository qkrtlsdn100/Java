package staticex;

public class Student {
	
	static int setialNum = 10000;
	
	int studentID;
	String studentName;
	
	public Student() {
		
		setialNum++;
		studentID = setialNum;
		
	}
	
	public static int getsetialNum() {
		int i = 10; // 지역변수
		i++;
		System.out.println(i);
		
//		studentName = "박신우"; //멤버변수, 인스턴스 변수
		
		return setialNum;	//static 변수, 클래스 변수
	}
}
