package review;

public class Subway {
	
	String subwayNumber;
	int passengerCount;
	int money;
	
	public Subway(String subwayNum) {
		this.subwayNumber = subwayNum;
		
	}
	
	public void take(int money) {
		this.money += money;
		passengerCount++;
		
	}
	
	
	public void showinfor() {
		System.out.println("지하철 " + subwayNumber + "의 승객은 " + passengerCount + "이며, 수입은 " + money + "입니다");
	}
}
