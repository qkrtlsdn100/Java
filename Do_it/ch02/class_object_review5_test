package review;

public class Test {
	public static void main(String[] args) {
		
		Student Sin = new Student("Sin", 19, 36800);
		Student Woo = new Student("Woo", 19, 23000);
		
		Taxi taxi = new Taxi("서울 08 별 14");
		
		Sin.taxi(taxi);
		Woo.taxi(taxi);
		
		taxi.showtaxiinfor();
		Sin.showstudentinfor();
		Woo.showstudentinfor();;
		
		System.out.println();
		
		Bus bus = new Bus(370);
		Sin.bus(bus);
		Woo.bus(bus);
		
		bus.showbusinfor();
		Sin.showstudentinfor();
		Woo.showstudentinfor();
	
		System.out.println();
		
		Subway subway = new Subway(5);
		Sin.subway(subway);
		Woo.subway(subway);
		
		subway.showsubwayinfor();
		Sin.showstudentinfor();
		Woo.showstudentinfor();
		
	}
}
