package Inheritance;

public class CustomerTest1 {
	
	public static void main(String[] args) {
		
		Customer customerSin = new Customer();
		customerSin.setCustomerID(10100);
		customerSin.setCustomerName("Sin");
		
		VIPCustomer customerWoo = new VIPCustomer();
		customerWoo.setCustomerID(10101);
		customerWoo.setCustomerName("Woo");
		
		customerWoo.bonusPoint = 1000;
		
		System.out.println(customerSin.showCustomerInfor());
		System.out.println(customerWoo.showCustomerInfor());
	}
}
