package constructor;

public class Person {
	
	String yourName;
	float height;
	float weight;
	
	public void showyourinfor() {
		System.out.println(yourName + " " + height + " " + weight);
		
	}
	public static void main(String[] args) {
		Person person = new Person();
		person.yourName = "박짱구";
		person.height = 150;
		person.weight = 40;
		
		person.showyourinfor();
	}
}
