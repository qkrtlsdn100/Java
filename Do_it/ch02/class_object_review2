package review;

public class class_object_review2 {
	
	String title;
	String artist;
	String album;
	String composer;
	int year;
	int track;
	
	public void showsong() {
		System.out.println("노래 제목 : " + title);
		System.out.println("아티스트 : " + artist);
		System.out.println("앨범 명 : " + album);
		System.out.println("작곡가 : " + composer);
		System.out.println("노래 발표년도 : " + year);
		System.out.println("트랙No. : " + track);
	}
	
	public static void main(String[] args) {
		class_object_review2 song = new class_object_review2();
		song.title = "Dancing queen";
		song.artist = "ABBA";
		song.album = "Arrival";
		song.composer = "ABBA";
		song.year = 2016;
		song.track = 1;
		
		song.showsong();
	}
}
