package review;

public class class_object_review2_test {
	
	String title;
	String artist;
	String album;
	String composer;
	int year;
	int track;
	
	public void showsong() {
		System.out.println("노래 제목 : " + title);
		System.out.println("아티스트 : " + artist);
		System.out.println("앨범 명 : " + album);
		System.out.println("작곡가 : " + composer);
		System.out.println("노래 발표년도 : " + year);
		System.out.println("트랙No. : " + track);
	}
	
	public class_object_review2_test(String title, String artist, String album, String composer, int year, int track) {
		this.title = title;
		this.artist = artist;
		this.album = album;
		this.composer = composer;
		this.year = year;
		this.track = track;
	}
	
	public static void main(String[] args) {
		class_object_review2_test abba = new class_object2_test("Dancing Queen", "ABBA", "Arrival", "ABBA", 2016, 1);
		abba.showsong();
	}
}
