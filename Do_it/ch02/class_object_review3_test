package review;

public class studentTest {
	public static void main(String[] args) {
		
		student studentSin = new student(100, "Sin");
		studentSin.setchemistry("물리", 64);
		studentSin.setphysics("화학", 95);
		
		student studentWoo = new student(101, "Woo");
		studentWoo.setchemistry("물리", 92);
		studentWoo.setphysics("화학", 60);
		
		studentSin.showstudentinfor();
		studentWoo.showstudentinfor();
	}
}
