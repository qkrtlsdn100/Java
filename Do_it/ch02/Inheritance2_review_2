package Employee;

public class employeeNum extends employee{
	
	private int employNumber;
	
	public employeeNum(String employname, String employeerank, String department, int employnum) {
		super(employname, employeerank, department);
		
		this.employNumber = employnum;
	}
	
	public void showEmploymentInfor() {
		System.out.println("사원번호 : " + employNumber + "\n" + employeeName + "님의 직급은 " + employeeRank + "이고, 부서는 " + Department + "입니다");
		
	}
	
}
